pipeline{
    agent any
    environment{
    PATH = "/opt/maven3.6.3/bin:$PATH"
        }
    stages{
        stage("SCM Checkout"){
            steps{
                git 'https://github.com/Patilnpriya/helloworld'
            }
        }
        stage("Maven Build"){
         steps{
         def mvnHome = tool name: 'Maven', type: 'maven'
          sh "${mvnHome}/bin/mvn clean package"
           }
        }
        
    }
}
